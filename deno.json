{
  "version": "1.5.0",
  "nodeModulesDir": "auto",
  "license": "MIT",
  "tasks": {
    "dev": "deno run -P --watch main.ts",
    "run": "deno run -P main.ts",
    "docker-run": "deno run  -P main.ts",
    "node-bundle": "deno bundle main.ts --minify --outdir dist"
  },
  "imports": {
    "@atproto/api": "npm:@atproto/api@^0.17.7",
    "@std/assert": "jsr:@std/assert@^1.0.15",
    "sanitize-html": "npm:sanitize-html@^2.17.0"
  },
  "permissions": {
    "default": {
      "env": true,
      "net": true,
      "read": [
        "./data/"
      ],
      "write": [
        "./data/"
      ]
    }
  }
}